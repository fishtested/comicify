{
  "manifest_version": 3,
  "name": "Comicify",
  "version": "1.0",
  "description": "An extension that changes all fonts to Comic Sans MS.",

  "icons": {
    "48": "Comicify.png"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["comicify.js"],
      "run_at": "document_start"
    }
  ]
}
